{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
        "no-underscore-dangle": ["error", { "allow": ["__"] }],
        "no-duplicate-imports": [0],
        "keyword-spacing": "error",
        "arrow-body-style": [0],
        "indent": ["error", 4],
        "react/jsx-indent-props": ["error", 4],
        "react/jsx-indent": ["error", 4],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    }
}
